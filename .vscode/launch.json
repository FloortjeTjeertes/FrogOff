    "version": "0.2.0",
    "configurations": [
        {
            "type": "alchemy65",
            "request": "attach",
            "name": "Debug rom with mesen",
            "dbgPath": "${workspaceFolder}/bin/game.dbg",
            "romPath": "${workspaceFolder}/bin/game.nes",
