        {
            "type": "alchemy65",
            "request": "launch",
            "name": "Debug rom with mesen",
            "romPath": "${workspaceFolder}/bin/game.nes",
            "dbgPath": "${workspaceFolder}/bin/game.dbg",
            "program": "${workspaceFolder}/tools/mesen.exe",
            "sourcePath": "${workspaceFolder}/",
            "stopOnEntry": false,
            "resetOnEntry": true
        }
        


        


    ]
}